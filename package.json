{
	"name": "elec",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"types": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"push": "drizzle-kit push",
		"export": "wrangler d1 export elec --remote --output elec.sql --table elec",
		"import": "wrangler d1 execute elec --file elec.sql --local"
	},
	"dependencies": {
		"@opennextjs/cloudflare": "^1.9.1",
		"@radix-ui/react-select": "^2.2.6",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.44.6",
		"lucide-react": "^0.542.0",
		"next": "15.4.6",
		"next-themes": "^0.4.6",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"recharts": "2.15.4",
		"tailwind-merge": "^3.3.1"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@tailwindcss/postcss": "^4.1.14",
		"@types/node": "^24.6.2",
		"@types/react": "^19.2.0",
		"@types/react-dom": "^19.2.0",
		"drizzle-kit": "^0.31.5",
		"eslint": "^9.37.0",
		"eslint-config-next": "15.4.6",
		"tailwindcss": "^4.1.14",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"wrangler": "^4.42.0"
	}
}