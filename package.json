    "chart.js": "^4.5.1",
    "daisyui": "^5.5.8",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.0",
    "es-toolkit": "^1.42.0",
    "hono": "^4.10.7",
    "tailwindcss": "^4.1.17",
